year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Misc",
         title = "galacticEdTools: An R Package for K-16 Education and Outreach",
         author = c(person("Matthew", "Wilkins", "R.")),
         year = year,
         note = note,
         url = c("https://galacticpolymath.github.io/galacticEdTools/"),
         header = "To cite the galacticEdTools R package:"
)
